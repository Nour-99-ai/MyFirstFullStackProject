    <script>
        $("#checkall").change(function () {
      var checked = $(this).is(":checked");
      if (checked) {
          $(".custom-checkbox").each(function () {
              $(this).prop("checked", true);
          });
  
      } else {
          $(".custom-checkbox").each(function () {
              $(this).prop("checked", false);
          });
      }
  });
      </script>
